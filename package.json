{
  "name": "online-help",
  "displayName": "Online Help",
  "description": "Context sensitive online help",
  "version": "0.2.8",
  "publisher": "salbert",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "lint": "eslint . --ext .ts,.tsx",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "publish": "vsce publish"
  },
  "icon": "images/icon.png",
  "keywords": [
    "online",
    "help",
    "rxjs",
    "typescript",
    "css"
  ],
  "galleryBanner": {
    "color": "#252627",
    "theme": "dark"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other",
    "Formatters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "online-help.openLink",
        "title": "Open online help"
      },
      {
        "command": "online-help.openClipboardLink",
        "title": "Open help about clipboard text"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "online-help.openLink",
          "key": "ctrl+shift+f1",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "online-help.openLink",
        "key": "ctrl+shift+f1"
      },
      {
        "command": "online-help.openClipboardLink",
        "key": "ctrl+shift+f2"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Online help configuration",
      "properties": {
        "online-help.enableRxjs": {
          "type": "boolean",
          "default": true,
          "description": "When enabled, shows help for rxjs operators."
        },
        "online-help.enableTypescript": {
          "type": "boolean",
          "default": true,
          "description": "When enabled, shows help for typescript keywords."
        },
        "online-help.enableDotnet": {
          "type": "boolean",
          "default": true,
          "description": "When enabled, shows help for dotnet."
        },
        "online-help.devdocs.enableDevdocs": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "When enabled, shows help on [devdocs.io](https://devdocs.io)."
        },
        "online-help.devdocs.setDevdocsLang": {
          "type": "string",
          "default": "",
          "markdownDescription": "Manually sets the search language for [devdocs.io](https://devdocs.io). If Online Help is not finding your language on [devdocs.io](https://devdocs.io), enter the name of the language shown on [devdocs.io](https://devdocs.io). Only applies when `#online-help.devdocs.enableDevdocs#` is checked. "
        },
        "online-help.enableFallback": {
          "type": "boolean",
          "default": true,
          "description": "When enabled, shows google search results"
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^12.12.68",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "typescript": "^4.0.3",
    "@types/vscode": "^1.40.0"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/s-albert/online-help/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/s-albert/online-help.git"
  }
}
